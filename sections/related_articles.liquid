<div class="padding-global padding-section-medium"><div class="container-large-2">{% assign currentArticle = article.title %}
<h2 class="heading-style-h2-4">
{{ section.settings.text_headline }}
</h2>
<div class="spacer-medium"></div><div class="blog_list-wrapper w-dyn-list"><div li-for:inside="article in blog.articles limit: 3" role="list" class="blog_list-2 w-dyn-items">
        {% for article in blog.articles limit: 3 %}                            
                            {% if article.title != currentArticle %}    <div li-if="article.title != currentArticle" role="listitem" class="blog_list-item w-dyn-item">
        </div>
{% endif %}
                            
                    {% endfor %}    </div>
<div class="hide w-dyn-empty"></div></div></div></div> {% schema %} {"tag":"section","name":"Related Articles","class":"section_related-articles","settings":[{"id":"text_headline","type":"text","label":"Headline","default":"Other Articles"}],"presets":[{"name":"Related Articles","category":"Liquify","blocks":[]}]} {% endschema %}